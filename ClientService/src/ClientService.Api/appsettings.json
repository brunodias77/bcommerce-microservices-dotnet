{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD};Port=${DB_PORT}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "${LOG_LEVEL_DEFAULT}",
      "Microsoft.AspNetCore": "${LOG_LEVEL_ASPNET}"
    }
  },
  "AllowedHosts": "${ALLOWED_HOSTS}",
  "Keycloak": {
    "Authority": "${KEYCLOAK_AUTHORITY}",
    "Audience": "${KEYCLOAK_AUDIENCE}",
    "ClientId": "${KEYCLOAK_CLIENT_ID}",
    "ClientSecret": "${KEYCLOAK_CLIENT_SECRET}",
    "AdminBaseUrl": "${KEYCLOAK_ADMIN_BASE_URL}",
    "AdminUsername": "${KEYCLOAK_ADMIN_USERNAME}",
    "AdminPassword": "${KEYCLOAK_ADMIN_PASSWORD}",
    "AdminClientId": "${KEYCLOAK_ADMIN_CLIENT_ID}",
    "Realm": "${KEYCLOAK_REALM}",
    "RequireHttpsMetadata": "${KEYCLOAK_REQUIRE_HTTPS_METADATA}",
    "ValidateAudience": "${KEYCLOAK_VALIDATE_AUDIENCE}",
    "ClockSkew": "${KEYCLOAK_CLOCK_SKEW}"
  }
}

